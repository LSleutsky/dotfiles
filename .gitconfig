[user]
	name = Lush Sleutsky
	email = lushsleutsky@gmail.com
	username = LSleutsky
[core]
	editor = code --wait
[diff]
	guitool = vscode
	tool = vscode
[difftool "vscode"]
  cmd = code --wait --diff $LOCAL $REMOTE
[merge]
  tool = vscode
[mergetool "vscode"]
  cmd = code --wait $MERGED
[fetch]
	prune = true
[web]
	google-chrome
[commit]
	verbose = true
[pull]
	rebase = true
[checkout]
	defaultRemote = origin
[init]
	defaultBranch = main
