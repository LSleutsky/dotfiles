[user]
	name = Lush Sleutsky
	email = lushsleutsky@gmail.com
	username = LSleutsky
[core]
	editor = nvim
[diff]
  guitool = nvimdiff
	tool = nvimdiff
[difftool "nvimdiff"]
  cmd = nvim -d \"$LOCAL\" \"$REMOTE\"
  prompt = true
[merge]
  tool = nvim
[mergetool]
  prompt = false
[mergetool "nvim"]
  cmd = nvim -f -c \"Gdiffsplit!\" \"$MERGED\"
[checkout]
	defaultRemote = origin
[commit]
	verbose = true
[fetch]
	prune = true
[pull]
	rebase = true
[init]
	defaultBranch = main
[web]
	google-chrome
